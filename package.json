    "@loadable/server": "^5.11.0",
    "babel-loader": "^8.0.6",
    "core-js": "3",
    "koa": "^2.11.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "less": "^3.10.3",
    "nunjucks": "^3.2.0",
